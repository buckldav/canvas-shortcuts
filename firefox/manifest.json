{
  "description": "Cycle through gradebook assignments with arrow keys (L and R)",
  "manifest_version": 2,
  "name": "Canvas Shortcuts",
  "version": "1.0",
  "homepage_url": "https://github.com/buckldav/canvas-shortcuts/tree/master/firefox",
  "icons": {
    "48": "icons/icon-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.instructure.com/*"],
      "js": ["canvas-arrows.js"]
    }
  ]
}

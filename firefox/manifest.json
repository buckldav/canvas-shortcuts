{
  "description": "Helpful shortcuts for Canvas SpeedGrader.",
  "manifest_version": 2,
  "name": "Canvas Shortcuts",
  "version": "1.3",
  "homepage_url": "https://github.com/buckldav/canvas-shortcuts/tree/master/firefox",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.instructure.com/*"],
      "js": ["canvas-shortcuts.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "Canvas Shortcuts",
    "default_popup": "index.html"
  }
}
